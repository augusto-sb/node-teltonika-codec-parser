{
  "name": "teltonika-codec-parser",
  "version": "1.0.2",
  "description": "decoder for buffers of teltonika codec 8, 8E and 16, UDP/TCP packages",
  "scripts": {
    "build": "tsc --build --clean && rm dist/index.d.ts -f && tsc",
    "test": "ts-node test/test.ts",
    "publish-script": "npm run build && npm publish"
  },
  "homepage": "https://github.com/augusto-sb/node-teltonika-codec-parser",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "keywords": [
    "teltonika"
  ],
  "browser": {
    "./dist/index.mjs": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "author": "augusto saldivar",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.8",
    "@types/node": "^25.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist/"
  ]
}
